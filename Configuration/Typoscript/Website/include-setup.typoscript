plugin.tx_powermail {
        view {
                templateRootPaths {
                        0 = EXT:powermail/Resources/Private/Templates/
                        1 = fileadmin/templates/powermail/Templates/
                }
                partialRootPaths {
                        0 = EXT:powermail/Resources/Private/Partials/
                        1 = fileadmin/templates/powermail/Partials/
                }
                layoutRootPaths {
                        0 = EXT:powermail/Resources/Private/Layouts/
                        1 = fileadmin/templates/powermail/Layouts/
                }
        }
}
plugin.tx_powermail.view {
        templateRootPath = fileadmin/templates/onepager/powermail/Templates/
        partialRootPath = fileadmin/templates/onepager/powermail/Partials/
        layoutRootPath = fileadmin/templates/onepager/powermail/Layouts/
}

